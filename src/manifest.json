{
  "manifest_version": 3,
  "name": "Maat",
  "description": "Automatically open Google Meet pages of the meeting about to start",
  "version": "0.0.1",
  "background": {
    "service_worker": "sw.js"
  },
  "permissions": [
    "alarms",
    "tabs"
  ],
  "host_permissions": [
    "https://calendar.google.com/*"
  ]
}
